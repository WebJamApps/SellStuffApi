{
  "name": "sell-stuff-api",
  "version": "0.0.2",
  "description": "The backend for SellStuff",
  "main": "index.js",
  "engines": {
    "node": ">=18.13.0 <19",
    "npm": ">=9.2.0 <10"
  },
  "files": [
    "lib/",
    "model/",
    "auth/",
    "config.js",
    "routes.js"
  ],
  "license": "MIT",
  "scripts": {
    "rmrf": "rm -rf build && rm -rf SellStuff && rm -rf coverage",
    "start:debug": "rm -rf build && tsc && npm-run-all -p ts-start ts-watch",
    "ts-watch": "tsc -w",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "ts-start": "DEBUG=SellStuffApi:* nodemon --trace-deprecation build/index.js",
    "build:front": "./postinstallC-I-C.sh && rm -rf ./SellStuff/src && rm -rf ./SellStuff/test",
    "postinstall": "npm run rmrf && tsc && npm run build:front",
    "rebuild:front": "./postinstallC-I-C.sh && npm run build:prod",
    "build:prod": "(cd SellStuff || exit; npm start -- build)",
    "start": "node build/index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && npm cache clean --force && npm install",
    "node:debug": "node --inspect-brk index.js",
    "test": "eslint ./src && rm -rf coverage && npm run test:jest",
    "test:lint": "eslint ./src --fix",
    "test:local": "npm run test:lint && rm -rf coverage && npm run test:jest && npm run cc",
    "test:jest": "jest --no-cache --config jest.config.json --runInBand --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/SellStuffApi.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.16",
    "@types/express-sslify": "^1.2.2",
    "@types/morgan": "^1.9.4",
    "@types/superagent": "^4.1.16",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.2",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-sslify": "^1.2.0",
    "helmet": "^6.0.1",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.4",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "superagent": "^8.0.9",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5"
  }
}
